{
  "expo": {
    "name": "almudarris",
    "slug": "almudarris",
    "version": "1.0.0",
    "scheme": "almudarris",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ic_launcher.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/images/teacher.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash_image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher.png",
        "backgroundColor": "#ffffff"
      }
    },
    "newArchEnabled": true
  }
}
